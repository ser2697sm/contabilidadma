generator client {
  provider = "prisma-client-js"
 // output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DB_PRODUCCION")
}

model Nacionalidades {
  id                Int     @id @default(autoincrement())
  nombre            String? 
  primer_apellido   String?
  segundo_apellido  String?
  nacionalidad      String?
  n_expediente      String?
  n_documento       String?
  fecha_examen      String?
  fecha_presentacion String?
  donde_vive        String?
  telefono          String?
  estado            String?
  consulta          String?
  pago_total        Int?
  primer_pago       Int?
  segundo_pago      Int?
  tercer_pago       Int?
  saldo             Int?
  notas             String?
}

model NacionalidadesOpcion {
  id Int @id @default(autoincrement())
  nombre String?
  primer_apellido String?
  segundo_apellido String?
  telefono String?
  nacionalidad String?
  progrenitor String?
  datos_hijos String?
  nif String?
  lugar String?
  consultado String?
  enviado Boolean?
  saldo Int?
  primer_pago Int?
  segundo_pago Int?
  tercer_pago Int?
  saldo_restante Int?
  notas String?
}

